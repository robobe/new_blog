{
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "workbench.editor.enablePreview": true,
    "[shellscript]": {
        "editor.insertSpaces": false,
        "editor.tabSize": 4,
        "editor.detectIndentation": false
    },
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "cSpell.words": [
        "pixhawk"
    ]
}